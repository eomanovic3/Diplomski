@model codeApp.Models.Contact

@{
    ViewBag.Title = "Create";
}
<section id="contact">
  
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="contact_area">
                    <div class="client_title">
                        <hr>
                        <h2>We Would <span>Love To Hear From You</span></h2>
                    </div>
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="contact_left wow fadeInLeft">
                                <form class="submitphoto_form">
                                    @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control wpcf7-text",placeholder="Your name" } })<br />
                                    @Html.EditorFor(model => model.Email, new { htmlAttributes = new { @class = "form-control wpcf7-text", placeholder = "Email" } })<br />
                                    @Html.EditorFor(model => model.Subject, new { htmlAttributes = new { @class = "form-control wpcf7-text", placeholder = "Subject" } })
                                    @Html.EditorFor(model => model.Tell, new { htmlAttributes = new { @class = "form-control wpcf7-text", placeholder = "What would you like to tell us" } })

                                      <input type="submit" value="Submit" class="wpcf7-submit photo-submit">
                                </form>
                            </div>
                        </div>
                       
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
